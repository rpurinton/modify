{
    "name": "modify",
    "version": "1.0.6",
    "type": "module",
    "description": "A modern Discord.js app using OpenAI Moderations Endpoint",
    "main": "modify.js",
    "scripts": {
        "start": "node modify.js",
        "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
    },
    "keywords": [
        "discord",
        "discord.js",
        "bot",
        "template",
        "skeleton",
        "locales",
        "events",
        "commands"
    ],
    "author": "Russell Purinton <russell.purinton@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/rpurinton/modify.git"
    },
    "bugs": {
        "url": "https://github.com/rpurinton/modify/issues"
    },
    "license": "MIT",
    "dependencies": {
        "discord.js": "^14.19.3",
        "dotenv": "^16.5.0",
        "mysql2": "^3.14.1",
        "openai": "^5.0.1",
        "winston": "^3.17.0"
    },
    "devDependencies": {
        "jest": "^29.7.0"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/?(*.)+(spec|test).mjs"
        ]
    }
}